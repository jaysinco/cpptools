{
    "workbench.startupEditor": "newUntitledFile",
    "workbench.iconTheme": "vscode-icons",
    "window.newWindowDimensions": "offset",
    "window.menuBarVisibility": "hidden",
    "window.title": "${dirty}${rootName}${separator}${profileName}${separator}${appName}",
    "window.titleBarStyle": "custom",
    "editor.fontFamily": "'Hack Nerd Font Mono'",
    "editor.fontWeight": 500,
    "editor.fontLigatures": false,
    "editor.fontSize": 16,
    "editor.formatOnSave": true,
    "editor.inlayHints.enabled": "offUnlessPressed",
    "editor.stickyScroll.enabled": false,
    "editor.codeLens": false,
    "diffEditor.ignoreTrimWhitespace": false,
    "files.insertFinalNewline": false,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "terminal.external.windowsExec": "%USERPROFILE%/ProgramFiles/Microsoft.WindowsTerminal/WindowsTerminal.exe",
    "terminal.integrated.profiles.windows": {
        "MSYS2 Bash": {
            "path": [
                "C:/msys64/msys2_shell.cmd"
            ],
            "args": [
                "-defterm",
                "-here",
                "-no-start",
                "-mingw64",
                "-use-full-path"
            ]
        }
    },
    "terminal.integrated.defaultProfile.windows": "MSYS2 Bash",
    "terminal.integrated.persistentSessionReviveProcess": "never",
    "terminal.integrated.fontFamily": "'CaskaydiaMono Nerd Font'",
    "terminal.integrated.fontSize": 15,
    "explorer.compactFolders": false,
    "explorer.confirmDelete": false,
    "explorer.autoReveal": false,
    "extensions.ignoreRecommendations": true,
    "extensions.autoUpdate": false,
    "extensions.autoCheckUpdates": false,
    "update.mode": "none",
    "update.showReleaseNotes": false,
    "security.workspace.trust.untrustedFiles": "open",
    "security.workspace.trust.enabled": false,
    "vsicons.dontShowNewVersionMessage": true,
    "C_Cpp.autoAddFileAssociations": false,
    "C_Cpp.intelliSenseEngine": "disabled",
    "dart.enableSdkFormatter": true,
    "dart.runPubGetOnPubspecChanges": "never",
    "dart.openDevTools": "never",
    "dart.devToolsBrowser": "default",
    "git.autofetch": false,
    "git.confirmSync": false,
    "git.openRepositoryInParentFolders": "always",
    "git.autoRepositoryDetection": false,
    "clangd.arguments": [
        "--completion-style=detailed",
        "--function-arg-placeholders=0",
        "--header-insertion=never"
    ],
    "[markdown]": {
        "editor.wordWrap": "off"
    },
    "MATLAB.indexWorkspace": false,
    "MATLAB.matlabConnectionTiming": "never",
    "MATLAB.telemetry": false,
    "find-it-faster.general.useTerminalInEditor": true,
    "find-it-faster.general.killTerminalAfterUse": true,
    "chat.commandCenter.enabled": false,
    "continue.showInlineTip": false,
    "continue.pauseCodebaseIndexOnStart": true,
    "codingcopilot.enableInlineChat": false,
    "codingcopilot.enableInlineChatAutoFormatCode": false,
    "codingcopilot.enableCodelens": false,
    "codingcopilot.modelResponseLanguage": "English",
    "codingcopilot.commitMessageLanguage": "en"
}